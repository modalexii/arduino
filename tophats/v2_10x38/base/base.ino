int Wheel(byte WheelPos) {
  
  int r, g, b, color;
  WheelPos = 255 - WheelPos;
  if(WheelPos < 85) {
    //color = (255 - WheelPos * 3, 0, WheelPos * 3);
    color = ( ( 255 - WheelPos * 3 << 16 ) | ( 0 << 8 ) | WheelPos * 3 );
  }
  if(WheelPos < 170) {
    WheelPos -= 85;
    //color = (0, WheelPos * 3, 255 - WheelPos * 3);
    color =  ( ( 0 << 16 ) | ( WheelPos * 3 << 8 ) |  255 - WheelPos * 3 );
  }
  else {
    WheelPos -= 170;
    //color = (WheelPos * 3, 255 - WheelPos * 3, 0);
    color = ( ( WheelPos * 3 << 16 ) | ( 255 - WheelPos * 3 << 8 ) | 0 );
  }

  r = ((color >> 16) & 0xFF) * luma;
  g = ((color >> 8) & 0xFF) * luma;
  b = (color & 0xFF) * luma;
  return ( ( r << 16 ) | ( g << 8 ) | b );
  
}

