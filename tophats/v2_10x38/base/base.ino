void pattern8() {

  // Pac Man

  for(int rpt = 0; rpt < 2; rpt++) {
    if(showPattern8) {
      for(int chase = 0; chase < MATRIX_WIDTH; chase++) {
        marquee(pacmanOpen, 1, 150, 0);
        for(int row = 0; row < MATRIX_HEIGHT; row++) {
          rotate(pacmanClosed[row], 2);
        }
        marquee(pacmanClosed, 1, 150, 0);
        for(int row = 0; row < MATRIX_HEIGHT; row++) {
          rotate(pacmanOpen[row], 2);
        }
      }
      for(int chase = 0; chase < MATRIX_WIDTH; chase++) {
        marquee(pacmanPersuitOpen, 1, 150, 1);
        for(int row = 0; row < MATRIX_HEIGHT; row++) {
          rotate(pacmanClosed[row], -2);
        }
        marquee(pacmanPersuitClosed, 1, 150, 1);
        for(int row = 0; row < MATRIX_HEIGHT; row++) {
          rotate(pacmanOpen[row], -2);
        }
      }
    }
  }
  
  leds.begin();
  
}

void loop() {

  if(!(brightnessSet)) {
    luma = 0.0000040 * sq(analogRead(A3)) + 0.01;
    brightnessSet = 1;
    Serial.print("setting luma to ");
    Serial.println(luma);
  }
  
  if(digitalRead(5) == LOW) {
    showPattern1 = 1;
    pattern1();
  }

  if(digitalRead(6) == LOW) {
    showPattern2 = 1;
    pattern2();
  }

  if(digitalRead(7) == LOW) {
    showPattern3 = 1;
    pattern3();
  }

  if(digitalRead(8) == LOW) {
     showPattern4 = 1;
     pattern4();
  }

  if(digitalRead(9) == LOW) {
     showPattern5 = 1;
     pattern5();
  }

  if(digitalRead(10) == LOW) {
     showPattern6 = 1;
     pattern6();
  }

  if(digitalRead(11) == LOW) {
     showPattern7 = 1;
     pattern7();
  }

  if(digitalRead(12) == LOW) {
     showPattern8 = 1;
     pattern8();
  }

}
