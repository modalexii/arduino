void bufferScreen(int pixelArray[][MATRIX_WIDTH]) {

  // given a 2d array representing the display, set the pixels for display
  // for non-zig-zag wiring (row n px -1 wired to row n+1 px 1)

  int pixel = 0;

  for (int row = 0; row < MATRIX_HEIGHT; row++) {
    for (int col = 0; col < MATRIX_WIDTH; col++) {
      leds.setPixel(pixel, pixelArray[row][col]);
      pixel++;
    }
  }

  // don't update the display here, so that we can draw on top of bitmaps parsed
  // by this function and then manually trigger the update
  //leds.show();

}

void bufferScreenSnake(int pixelArray[][MATRIX_WIDTH]) {

  // given a 2d array representing the display, set the pixels for display
  // for zig-zag wiring (row n px -1 wired to row n+1 px -1)

  int pixel, rgb, r, g, b = 0;

  for (int row = 0; row < MATRIX_HEIGHT; row++) {
    if (row % 2 == 0) {
      for (int col = 0; col < MATRIX_WIDTH; col++) {
        rgb = pixelArray[row][col];
        r = ((rgb >> 16) & 0xFF) * luma;
        g = ((rgb >> 8) & 0xFF) * luma;
        b = (rgb & 0xFF) * luma;
        leds.setPixel(pixel, r, g, b);
        pixel++;
      }
    } else {
      for (int col = MATRIX_WIDTH - 1; col >= 0; col--) {
        rgb = pixelArray[row][col];
        r = ((rgb >> 16) & 0xFF) * luma;
        g = ((rgb >> 8) & 0xFF) * luma;
        b = (rgb & 0xFF) * luma;
        leds.setPixel(pixel, r, g, b);
        pixel++;
      }
    }
  }

  // don't update the display here, so that we can draw on top of bitmaps parsed
  // by this function and then manually trigger the update
  //leds.show();

}

void reverse(int a[], int sz) {
  
  // supports rotate()
  
  int i, j;
  for (i = 0, j = sz; i < j; i++, j--) {
    int tmp = a[i];
    a[i] = a[j];
    a[j] = tmp;
  }
  
}

void rotate(int a[], int amt) {

  // circularly shift elements in a 1d array
  
  if (amt < 0) {
    amt = MATRIX_WIDTH + amt;
  }
  reverse(a, MATRIX_WIDTH-amt-1);
  reverse(a+MATRIX_WIDTH-amt, amt-1);
  reverse(a, MATRIX_WIDTH-1);
  
}

